
fn main() { 
    Function func = () -> {
            String str = "";
            int number = in_int (
                "Please enter a number: "
            );

            for (int i = 1; i <= number; i++) str = (i != number) ? str.concat(i + "\n") : str.concat("" + i);
            ret str;
    };

    println ("\n" +
        func
        .call()
        .toString()
    );
}