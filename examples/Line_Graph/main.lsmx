
fn main() {
    int[] xPoints = new double[101];
    int[] yPoints = new double[101];

    for (int i = -50; i <= 50; i++) xPoints[i + 50] = (i * 5); yPoints[i + 50] = (i * 5);

    new LGraph (
        xPoints,         // X-Points
        yPoints,         // Y-Points
        500,             // Preferred width
        500,             // Preffered height
        Color.BLACK      // Point and axes colour
    ).draw();
}